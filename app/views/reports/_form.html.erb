<%= simple_form_for(@report) do |f| %>
  <%= f.error_notification %>



  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :phone %>
   
    <%= f.input :subject %>
     </div>

    Select Type of bug
  <%= f.select :feed_type, [['Urgent','Urgent'],['Medium','medium'],['sugestion ','sugestion']]  %>
   
    <%= f.input :description %>
 

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
